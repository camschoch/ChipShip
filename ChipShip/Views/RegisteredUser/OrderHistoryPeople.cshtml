@model ChipShip.Models.ViewModels.OrderRequestViewModel
@{
    ViewBag.Title = "OrderHistoryPeople";
}

<h2>OrderHistoryPeople</h2>

<h2>ShoppingCart</h2>
<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>Name</th>
        <th>View Order</th>
    </tr>
    @foreach (var d in Model.Users)
    {
      
            <tr>
                <td>@Html.DisplayFor(temp => d.UserName)</td>
                <td>@Html.ActionLink("View Order", "OrderHistory", "RegisteredUser", new { userId = d.Id }, null)</td>
            </tr>        
    }
</table>